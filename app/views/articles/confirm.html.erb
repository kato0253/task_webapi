<h3>以下の内容で、送信する。</h3>
<%= form_with(model: @article, local: true , url: articles_path, method: confirm_form_method) do |form| %>
  <%= form.hidden_field :title %>
  <%= form.hidden_field :content %>
  <p>タイトル:<%= @article.title %></p>
  <p>本文:<%= @article.content %></p>
  <%= form.submit  "登録する" %>
  <%= form.submit "戻る", name: 'back' %>
<% end %>
